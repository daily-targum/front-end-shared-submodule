    "contentful": "7.14.6",
    "dayjs": "1.8.35",
    "graphql-tag": "2.11.0",
    "isomorphic-fetch": "^2.2.1"
  },
  "devDependencies": {
    "@babel/preset-env": "7.11.5",
    "@babel/preset-react": "7.10.4",
    "@babel/preset-typescript": "7.10.4",
    "@commitlint/config-conventional": "9.1.2",
    "@types/jest": "26.0.13",
    "babel-polyfill": "^6.26.0",
    "commitlint": "9.1.2",
    "dotenv": "^8.2.0",
    "husky": "^4.2.5",
    "jest": "26.4.2",
    "react-test-renderer": "^16.13.1",
    "typescript": "4.0.2"
  },
  "husky": {
    "hooks": {
